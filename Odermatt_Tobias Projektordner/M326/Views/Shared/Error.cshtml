@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
}

<h1 class="text-danger">Oh Nein.</h1>
<h2 class="text-danger">Ein Fehler ist aufgetreten, bei dem Versuch Ihre Anfrage zu verarbeiten.</h2>

@if (Model.ShowRequestId)
{
    <p>
        <strong>Request ID:</strong> <code>@Model.RequestId</code>
    </p>
}
